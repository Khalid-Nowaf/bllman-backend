{
  "name": "bllman-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "grunt": "./node_modules/.bin/grunt",
    "test": "npm run grunt && DEBUG= ./node_modules/.bin/mocha './test/**/*.spec.js' --reporter spec -r './bin/www'",
    "cover": "npm run grunt && ./node_modules/.bin/istanbul cover _mocha  './test/*.spec.js' --reporter -r './bin/www'",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "@types/body-parser": "0.0.33",
    "@types/es6-promise": "0.0.32",
    "@types/morgan": "^1.7.32",
    "@types/superagent": "^2.0.35",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "morgan": "~1.7.0"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-ts": "^6.0.0-beta.3",
    "grunt-tslint": "^3.3.0",
    "grunt-concurrent": "^2.3.1",
    "grunt-newer": "^1.2.0",
    "grunt-nodemon": "^0.4.2",
    "nodemon": "^1.11.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.6",
    "coveralls": "^2.11.14",
    "mocha": "^3.1.2",
    "@types/chai": "^3.4.34",
    "@types/express": "^4.0.33",
    "@types/mocha": "^2.2.32",
    "istanbul": "^0.4.5",
    "chai": "^3.5.0",
    "@types/supertest": "^1.1.31",
    "supertest": "^2.0.1"
  }
}
