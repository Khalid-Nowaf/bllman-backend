{
  "name": "bllman-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "grunt": "./node_modules/.bin/grunt",
    "test": "npm run grunt && DEBUG= ./node_modules/.bin/mocha './dest/test/**/*.spec.js' --reporter spec -r './bin/www'",
    "cover": "npm run grunt && ./node_modules/.bin/istanbul cover _mocha  './dest/test/**/*.spec.js' --reporter -r './bin/www'",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "@types/bcrypt": "0.0.30",
    "@types/body-parser": "0.0.33",
    "@types/es6-promise": "0.0.32",
    "@types/lodash": "^4.14.37",
    "@types/mongoose": "^4.5.39",
    "@types/morgan": "^1.7.32",
    "@types/superagent": "^2.0.35",
    "bcrypt": "^0.8.7",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "lodash": "^4.16.5",
    "mongoose": "^4.6.5",
    "mongoose-beautiful-unique-validation": "^5.1.1",
    "mongoose-validator": "^1.2.5",
    "morgan": "~1.7.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/express": "^4.0.33",
    "@types/mocha": "^2.2.32",
    "@types/supertest": "^1.1.31",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-newer": "^1.2.0",
    "grunt-nodemon": "^0.4.2",
    "grunt-ts": "^6.0.0-beta.3",
    "grunt-tslint": "^3.3.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "supertest": "^2.0.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.6"
  }
